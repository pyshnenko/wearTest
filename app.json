{
  "expo": {
    "name": "wearTest",
    "slug": "wearTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spamigorLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/spamigorLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.spamigor.wearTest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/spamigorLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/spamigorLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "54a48496-0f74-4cdc-af39-97d2cc124b6d"
      }
    }
  }
}
